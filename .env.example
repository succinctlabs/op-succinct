# Configuration for running the OP Proposer + SP1 Server
L1_RPC=
L1_BEACON_RPC=
L2_RPC=
L2_NODE_RPC=

# Contract + OP Proposer
PRIVATE_KEY=

# Contract
ETHERSCAN_API_KEY=

# OP Proposer
L2OO_ADDRESS=0x0E8d5C8041bc99E1869bdE4d7384AD00C41865b

# Proof Server
PROVER_NETWORK_RPC=
SP1_PRIVATE_KEY=
